      app:layout_constraintStart_toEndOf="@+id/imageButtonLocation2"
      app:layout_constraintTop_toBottomOf="@+id/textViewTackleCoarse"
      app:srcCompat="@drawable/compass" />

  <TextView
      android:id="@+id/textViewWeather"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="32dp"
      android:layout_marginRight="32dp"
      android:text="Weather"
      android:textColor="#FDFCFC"
      android:textSize="20sp"
      android:textStyle="bold"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageButtonWeather" />

  <TextView
      android:id="@+id/textViewTackleCoarse"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Tackle"
      android:textSize="20sp"
      android:textStyle="bold"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageButtonTackleCoarse" />

  <TextView
      android:id="@+id/textViewKnotsCoarse"
      android:layout_width="wrap_content"
