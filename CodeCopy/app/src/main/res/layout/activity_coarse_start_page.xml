      app:srcCompat="@drawable/facebook" />

  <ImageButton
      android:id="@+id/imageButtonGlossaryCoarse"
      android:layout_width="100dp"
      android:layout_height="100dp"
      android:layout_marginTop="10dp"
      android:scaleType="centerInside"
      app:layout_constraintEnd_toStartOf="@+id/imageButtonFaceBookCoarse"
      app:layout_constraintStart_toEndOf="@+id/imageButtonTorchCoarse"
      app:layout_constraintTop_toBottomOf="@+id/textViewCompassCoarse"
      app:srcCompat="@drawable/glossary" />

  <TextView
      android:id="@+id/textViewTorchCoarse"
      android:layout_width="wrap_content"
